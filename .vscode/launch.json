{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.drt.App",
            "projectName": "matsim-leeds"
        },
        {
            "type": "java",
            "name": "Run OSM Converter",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.pt2matsim.RunOSMConverter",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": "src/main/resources/config_pt2matsim.xml"
        },
        {
            "type": "java",
            "name": "Run GTFS to Transit Schedule",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.pt2matsim.RunPTConverter",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": [
                "data/external/study_area_gtfs_bus.zip",
                "20230814",
                "EPSG:3857",
                "data/supply/schedule_unmapped.xml.gz",
                "data/supply/vehicles_unmapped.xml"
            ]
        },
        {
            "type": "java",
            "name": "Run PT Mapper",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.pt2matsim.RunPTMapper",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": "src/main/resources/config_pt2matsim.xml"
        },
        {
            "type": "java",
            "name": "Create Default MATSim Config",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.RunCreateDefaultConfig",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": "src/main/resources/config_simulation.xml"
        },
        {
            "type": "java",
            "name": "Create Sample from Population",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.sample.CreatePopulationSample",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": [
                "data/demand/plans.xml",
                "scenarios/test/plans.xml"
            ]
        },
        {
            "type": "java",
            "name": "Run simulation with specified config",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.RunSimulation",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": "src/main/resources/config_simulation.xml"
        },
        {
            "type": "java",
            "name": "RunDMCSimulation",
            "request": "launch",
            "mainClass": "com.husseinmahfouz.matsim.dmc.RunDMCSimulation",
            "projectName": "matsim-leeds",
            "cwd": "${workspaceFolder}/matsim-leeds",
            "args": "--config-path src/main/resources/config_simulation_dmc.xml"
        }
    ]
}